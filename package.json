{
  "name": "@michaelcoxon/rest-client",
  "version": "1.3.9",
  "description": "A simple REST client.",
  "main": "dist/index.js",
  "module": "lib.2015/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "version": "npm -v && node -v",
    "run-tests": "mocha -r ts-node/register tests/**/*.unit.spec.ts",
    "test": "npm-run-all -s build-dev build-prod -p run-tests",
    "build-dev": "SET NODE_ENV=development&& webpack --no-color",
    "build-prod": "SET NODE_ENV=production&& webpack --no-color",
    "build-tests": "webpack --config ./webpack.config.tests.js --display-error-details",
    "build-doc": "typedoc src --out docs --mode modules --exclude \"**/node_modules\"",
    "prepack": "npm-run-all -s test build-doc copy-nojekyll",
    "copy-nojekyll": "copy \".nojekyll\" \"docs/.nojekyll\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelcoxon/rest-client.git"
  },
  "keywords": [
    "rest-client",
    "typescript"
  ],
  "author": "Michael Coxon",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/michaelcoxon/rest-client/issues"
  },
  "homepage": "https://github.com/michaelcoxon/rest-client#readme",
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.5",
    "@types/webpack-env": "1.13.6",
    "awesome-typescript-loader": "5.2.0",
    "chai": "4.1.2",
    "dts-bundle": "0.7.3",
    "file-loader": "1.1.11",
    "json-loader": "0.5.7",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "ts-node": "7.0.1",
    "typedoc": "0.11.1",
    "typescript": "3.0.1",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-cyclic-dependency-checker": "0.0.1",
    "xmlhttprequest": "1.8.0"
  },
  "dependencies": {
    "tslib": "1.9.3",
    "@michaelcoxon/utilities": "1.7.2"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "test"
    ]
  }
}
